{
  "name": "betta",
  "version": "0.0.0",
  "description": "Simple Bootstrap variable editor",
  "main": "static/js/index.js",
  "scripts": {
    "start": "./scripts/start",
    "build": "./scripts/build",
    "dist": "./scripts/dist",
    "build-js": "browserify -t babelify static/js/index.js -o static/build/js/index.js",
    "watch-js": "watchify -t babelify static/js/index.js -o static/build/js/index.js -v",
    "build-less": "lessc static/less/index.less > static/build/css/index.css && echo 'Less compiled'",
    "watch-less": "watch 'npm run build-less' static/less/",
    "lint": "eslint static/js/",
    "test": "npm run lint"
  },
  "keywords": [
    "css",
    "less",
    "betta",
    "bootstrap",
    "editor"
  ],
  "homepage": "http://betta.style/",
  "repository": {
    "type": "git",
    "url": "https://github.com/dabapps/betta"
  },
  "bugs": {
    "url": "https://github.com/dabapps/betta/issues"
  },
  "author": "Jake 'Sid' Smith",
  "contributors": [
    {
      "name": "Jake 'Sid' Smith",
      "url": "https://githib.com/JakeSidSmith"
    },
    {
      "name": "Emmanouil Konstantinidis",
      "url": "https://githib.com/ekonstantinidis"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.0.0-beta.54",
    "@babel/preset-react": "^7.0.0-beta.54",
    "babelify": "^9.0.0",
    "bootstrap": "=3.3.4",
    "browserify": "^16.2.2",
    "holderjs": "=2.8.0",
    "jquery": "^3.3.1",
    "less": "^3.8.0",
    "react": "=0.13.3",
    "underscore": "=1.8.3"
  },
  "devDependencies": {
    "concurrently": "^3.6.1",
    "eslint": "^3.19.0",
    "eslint-config-dabapps": "github:dabapps/eslint-config-dabapps",
    "http-server": "^0.11.1",
    "watch": "=0.16.0",
    "watchify": "^3.11.0"
  }
}
